config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Command",
"language": "GDScript",
"path": "res://Engine/Commands/Command.gd"
}, {
"base": "Button",
"class": "CommandButton",
"language": "GDScript",
"path": "res://Engine/Commands/CommandButton.gd"
}, {
"base": "Command",
"class": "SetTileCommand",
"language": "GDScript",
"path": "res://Engine/Commands/SetTileCommand.gd"
} ]
_global_script_class_icons={
"Command": "",
"CommandButton": "",
